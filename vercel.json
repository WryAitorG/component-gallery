{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/components/(.*)",
      "destination": "/frontend/components/[slug]"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ]
}
